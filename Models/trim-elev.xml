<?xml version="1.0"?>
  <PropertyList> 
    <path>trim-elev.ac</path>
    <animation> 
      <type>translate</type>
      <object-name>pointer</object-name>
      <property>controls/flight/elevator-trim</property>
      <interpolation>
	  <entry> 
          <ind>-1</ind>
          <dep>-0.010</dep>
        </entry> 
        <entry> 
          <ind>0</ind>
          <dep>0.0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>0.010</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.3</red>
        <green>0.025</green>
        <blue>0.025</blue>
      </emission>
    </animation>

  </PropertyList>
