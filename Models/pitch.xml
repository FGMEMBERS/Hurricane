<?xml version="1.0"?>
  <PropertyList> 
    <path>pitch.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Prop</object-name>
      <property>controls/engines/engine[0]/propeller-pitch</property>
      <interpolation> 
        <entry> 
          <ind>0.0</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>1.0</ind>
          <dep>45</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.3</red>
        <green>0.025</green>
        <blue>0.025</blue>
      </emission>
    </animation>
  </PropertyList>
