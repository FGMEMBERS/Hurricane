<?xml version="1.0"?>
  <PropertyList> 
    <path>boost.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>boost-Needle1</object-name>
      <property>engines/engine/boost-gauge-psi</property>
      <!--    <factor>0.4911</factor>
     <offset>-29.92</offset> -->
      <interpolation> 
        <entry> 
          <ind>-15</ind>
          <dep>-90</dep>
        </entry>
        <entry> 
          <ind>-10</ind>
          <dep>-45</dep>
        </entry>
        <entry> 
          <ind>-5</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>45</dep>
        </entry>
        <entry> 
          <ind>5</ind>
          <dep>90</dep>
        </entry>
        <entry> 
          <ind>10</ind>
          <dep>135</dep>
        </entry>
        <entry> 
          <ind>15</ind>
          <dep>180</dep>
        </entry>
        <entry> 
          <ind>20</ind>
          <dep>225</dep>
        </entry>
        <entry> 
          <ind>25</ind>
          <dep>270</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
    <!--<animation>
      <type>material</type>
      <condition>
        <greater-than>
          <property>/controls/lighting/panel-norm[1]</property>
          <value>0</value>
        </greater-than>
        <property>/sim/rendering/rembrandt/enabled</property>
      </condition>
      <emission>
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.0</red>
        <green>0.0</green>
        <blue>0.0</blue>
      </emission>
    </animation>-->

    <animation>
      <type>material</type>
      <condition>
        <greater-than>
          <property>/controls/lighting/panel-norm</property>
          <value>0</value>
        </greater-than>
        <not>
          <property>/sim/rendering/rembrandt/enabled</property>
        </not>
      </condition>
      <emission>
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
