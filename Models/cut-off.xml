<?xml version="1.0"?>
  <PropertyList> 
    <path>cut-off.ac</path>
    <animation> 
      <type>translate</type>
      <object-name>pull</object-name>
      <property>controls/engines/engine/cutoff-pull-norm</property>
      <factor>0.025</factor>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
