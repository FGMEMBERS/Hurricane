<?xml version="1.0"?>
<!--    <!DOCTYPE PropertyList SYSTEM "panel.dtd">

    This part should be the same for any 3D panel.  Customize the actions section -->
<PropertyList>
	<name>Hurricane Panel Hotspots</name>
	<background>Aircraft/Hurricane/Models/transparent-bg.rgb</background>
	<w>1024</w>
	<h>768</h>
	<y-offset>0</y-offset>
	<instruments>
		<instrument>
			<name>Front Panel Controls</name>
			<x>512</x>
			<y>384</y>
			<w>1024</w>
			<h>768</h>
			<w-base>1024</w-base>
			<h-base>1024</h-base>
			<layers>
				<layer>
					<name>background</name>
					<texture>
						<path>Aircraft/Hurricane/Models/transparent-bg.rgb</path>
						<x1>0.0</x1>
						<y1>0.0</y1>
						<x2>1.0</x2>
						<y2>1.0</y2>
					</texture>
				</layer>
				<!--<layer>
					<name>grid</name>
					<texture>
						<path>Aircraft/Hurricane/Models/hotspotgrid.rgb</path>
						<x1>0.0</x1>
						<y1>0.0</y1>
						<x2>1.0</x2>
						<y2>1.0</y2>
					</texture>
				</layer>-->
			</layers>
			<actions>
	<!-- altimeter hotspots -->
				<action>
					<name>calibrate fine</name>
					<button>0</button>
					<x>-145</x>
					<y>-130</y>
					<w>20</w>
					<h>30</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/altimeter/setting-inhg</property>
						<step>-0.01</step>
						<min>26.0</min>
						<max>33.0</max>
						<wrap>false</wrap>
					</binding>
				</action>
				<action>
					<name>calibrate coarse</name>
					<button>1</button>
					<x>-145</x>
					<y>-130</y>
					<w>20</w>
					<h>30</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/altimeter/setting-inhg</property>
						<step>-0.1</step>
						<min>26.0</min>
						<max>33.0</max>
						<wrap>false</wrap>
					</binding>
				</action>
				<action>
					<name>calibrate fine</name>
					<button>0</button>
					<x>-125</x>
					<y>-130</y>
					<w>20</w>
					<h>30</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/altimeter/setting-inhg</property>
						<step>0.01</step>
						<min>26.0</min>
						<max>33.0</max>
						<wrap>false</wrap>
					</binding>
				</action>
				<action>
					<name>calibrate coarse</name>
					<button>1</button>
					<x>-125</x>
					<y>-130</y>
					<w>20</w>
					<h>30</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/altimeter/setting-inhg</property>
						<step>0.1</step>
						<min>26.0</min>
						<max>33.0</max>
						<wrap>false</wrap>
					</binding>
				</action>
				<!-- magneto switch hotspots -->
				<action>
					<name>switch up left</name>
					<button>0</button>
					<x>-355</x>
					<y>-95</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>nasal</command>
						<script>

							hurricane.setleftMagswitch(1);

						</script>
					</binding>
				</action>
				<action>
					<name>switch down left</name>
					<button>0</button>
					<x>-355</x>
					<y>-125</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>nasal</command>
						<script>

							hurricane.setleftMagswitch(0);

						</script>
					</binding>
				</action>
				<action>
					<name>switch up right</name>
					<button>0</button>
					<x>-325</x>
					<y>-95</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>nasal</command>
						<script>

							hurricane.setrightMagswitch(1);

						</script>
					</binding>
				</action>
				<action>
					<name>switch down right</name>
					<button>0</button>
					<x>-325</x>
					<y>-125</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>nasal</command>
						<script>

							hurricane.setrightMagswitch(0);

						</script>
					</binding>
				</action>
				<!-- navigation light switch -->
				<action>
					<name>nav light up</name>
					<button>0</button>
					<x>-270</x>
					<y>-95</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/lighting/nav-lights</property>
						<value type="bool">1</value>
					</binding>
				</action>
				<action>
					<name>nav light down</name>
					<button>0</button>
					<x>-270</x>
					<y>-125</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/lighting/nav-lights</property>
						<value type="bool">0</value>
					</binding>
				</action>
				<!-- clock hotspots -->
				<action>
					<name>adjust left fine</name>
					<button>0</button>
					<x>-330</x>
					<y>-60</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/clock/offset-sec</property>
						<step>-60</step>
						<min>0</min>
						<max>43200</max>
						<wrap>true</wrap>
					</binding>
				</action>
				<action>
					<name>adjust left coarse</name>
					<button>1</button>
					<x>-330</x>
					<y>-60</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/clock/offset-sec</property>
						<step>-900</step>
						<min>0</min>
						<max>43200</max>
						<wrap>1</wrap>
					</binding>
				</action>
				<action>
					<name>adjust right fine</name>
					<button>0</button>
					<x>-310</x>
					<y>-60</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/clock/offset-sec</property>
						<step>60</step>
						<min>0</min>
						<max>43200</max>
						<wrap>true</wrap>
					</binding>
				</action>
				<action>
					<name>adjust right coarse</name>
					<button>1</button>
					<x>-310</x>
					<y>-60</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-adjust</command>
						<property>/instrumentation/clock/offset-sec</property>
						<step>900</step>
						<min>0</min>
						<max>43200</max>
						<wrap>1</wrap>
					</binding>
				</action>
				<!-- starter push -->
				<action>
					<name>starter push</name>
					<button>0</button>
					<x>-440</x>
					<y>-125</y>
					<w>35</w>
					<h>35</h>
					<binding>
						<command>nasal</command>
						<script>controls.startEngine(1)</script>
					</binding>
					<mod-up>
						<binding>
							<command>nasal</command>
							<script>controls.startEngine(0)</script>
						</binding>
					</mod-up>
				</action>
				<!-- fuel gauge switches -->
				<action>
					<name>push to read fuel</name>
					<button>0</button>
					<x>345</x>
					<y>75</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/fuel-gauge</property>
						<value type="bool">1</value>
					</binding>
					<mod-up>
						<binding>
							<command>property-assign</command>
							<property>/controls/switches/fuel-gauge</property>
							<value type="bool">0</value>
						</binding>
					</mod-up>
				</action>
				<action>
					<name>select tank[2]</name>
					<button>0</button>
					<x>345</x>
					<y>95</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/fuel-gauge-sel</property>
						<value type="int">2</value>
					</binding>
				</action>
				<action>
					<name>select tank[0]</name>
					<button>0</button>
					<x>325</x>
					<y>75</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/fuel-gauge-sel</property>
						<value type="int">0</value>
					</binding>
				</action>
				<action>
					<name>select tank[1]</name>
					<button>0</button>
					<x>365</x>
					<y>75</y>
					<w>20</w>
					<h>20</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/fuel-gauge-sel</property>
						<value type="int">1</value>
					</binding>
				</action>
				<!-- gunsight switches -->
				<action>
					<name>gun sight main up</name>
					<button>0</button>
					<x>240</x>
					<y>220</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gun-sight-main</property>
						<value type="bool">1</value>
					</binding>
				</action>
				<action>
					<name>gun sight main down</name>
					<button>0</button>
					<x>240</x>
					<y>190</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gun-sight-main</property>
						<value type="bool">0</value>
					</binding>
				</action>
