				<!-- gear indicator switches -->
				<action>
					<name>gear indicator main up</name>
					<button>0</button>
					<x>-260</x>
					<y>220</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator-main</property>
						<value type="bool">1</value>
					</binding>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator-alt</property>
						<value type="bool">0</value>
					</binding>
				</action>
				<action>
					<name>gear indicator alt down</name>
					<button>0</button>
					<x>-260</x>
					<y>190</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator-alt</property>
						<value type="bool">1</value>
					</binding>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator-main</property>
						<value type="bool">0</value>
					</binding>
				</action>
				<action>
					<name>gear indicator up</name>
					<button>0</button>
					<x>-290</x>
					<y>220</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator</property>
						<value type="bool">1</value>
					</binding>
				</action>
				<action>
					<name>gear indicator down</name>
					<button>0</button>
					<x>-290</x>
					<y>190</y>
					<w>30</w>
					<h>30</h>
					<binding>
						<command>property-assign</command>
						<property>/controls/switches/gear-indicator</property>
						<value type="bool">0</value>
					</binding>
				</action>
				<!-- boost control cutout -->
				<action>
					<name>toggle cutout</name>
					<button>0</button>
					<x>-420</x>
					<y>-60</y>
					<w>50</w>
					<h>55</h>
					<binding>
						<command>nasal</command>
						<script>

							hurricane.toggleCutout();

						</script>
					</binding>
				</action>
				<!-- supercharger control -->
				<action>
					<name>supercharger control</name>
					<button>0</button>
					<x>-370</x>
					<y>-282</y>
					<w>45</w>
					<h>60</h>
					<binding>
						<command>nasal</command>
						<script>
							hurricane.toggleBoost();
						</script>
					</binding>
				</action>
				<!-- primer pump -->
				<action>
					<name>primer pump</name>
					<button>0</button>
					<x>360</x>
					<y>-270</y>
					<w>45</w>
					<h>45</h>
					<repeatable type="bool">false</repeatable>
					<binding>
						<command>nasal</command>
						<script>
							fuel.pumpPrimer(1);
						</script>
					</binding>
					<mod-up>
						<binding>
							<command>nasal</command>
							<script>
								fuel.pumpPrimer(0);
							</script>
						</binding>
					</mod-up>
				</action>
