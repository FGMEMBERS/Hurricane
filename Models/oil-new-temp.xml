<?xml version="1.0"?>
  <!-- oil temp indicator deg F -->
  <PropertyList> 
    <path>oil-temp.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>rad-temp-Needle1</object-name>
      <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
      <interpolation> 
        <entry>
          <ind>0</ind>
          <dep>30</dep>
        </entry>
        <entry>
          <ind>20</ind>
          <dep>90</dep>
        </entry>
        <entry>
          <ind>40</ind>
          <dep>150</dep>
        </entry>
        <entry>
          <ind>60</ind>
          <dep>210</dep>
        </entry>
        <entry>
          <ind>80</ind>
          <dep>270</dep>
        </entry>
        <entry>
          <ind>100</ind>
          <dep>330</dep>
        </entry>
        <entry>
          <ind>110</ind>
          <dep>360</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
      <shininess>10</shininess>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
