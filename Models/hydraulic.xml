<?xml version="1.0"?>
<PropertyList>
  <path>hydraulic.ac</path>
  <!---->
  <animation>
    <type>rotate</type>
    <object-name>lever</object-name>
    <property>controls/hydraulic/lever[0]</property>
    <interpolation>
      <entry>
        <ind>-1</ind>
        <dep>-3.5</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>-1</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>1</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.078</x-m>
      <y-m>-0.059</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>lever</object-name>
    <property>controls/hydraulic/lever[1]</property>
    <interpolation>
      <entry>
        <ind>-1</ind>
        <dep>-12.5</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>12.5</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.078</x-m>
      <y-m>-0.059</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <!--<animation>
