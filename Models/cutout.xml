<?xml version="1.0"?>
  <PropertyList> 
    
	<path>cutout.ac</path>
    
	<animation> 
      <type>translate</type>
      <object-name>knob</object-name>
      <property>controls/engines/engine/boost-control-cutout</property>
      <factor>0.025</factor>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
