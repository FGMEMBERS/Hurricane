<?xml version="1.0"?>
<PropertyList>
	<path>fuel-cock.ac</path>

	<animation>
		<type>pick</type>
		<visible>false</visible>
		<object-name>pick-left</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					fuel.openCock(-1);
				</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<visible>false</visible>
		<object-name>pick-right</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					fuel.openCock(1);
				</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>lever</object-name>
		<property>controls/engines/engine/fuel-cock/lever</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>090</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2.0</ind>
				<dep>-90</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/controls/lighting/panel-norm</property>
				<value>0</value>
			</greater-than>
			<not>
				<property>/sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
</PropertyList>
