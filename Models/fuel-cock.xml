<?xml version="1.0"?>
<PropertyList>
  <path>fuel-cock.ac</path>
  <animation>
    <type>rotate</type>
    <object-name>lever</object-name>
    <property>controls/engines/engine/fuel-cock/lever</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>090</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>2.0</ind>
        <dep>-90</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <!--<animation>
