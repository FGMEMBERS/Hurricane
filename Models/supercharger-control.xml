<?xml version="1.0"?>
<PropertyList>
  <path>supercharger-control.ac</path>
  <animation>
    <type>translate</type>
    <object-name>knob</object-name>
    <property>controls/engines/engine/boost</property>
    <interpolation>
      <entry>
        <ind>0.79</ind>
        <dep>0.025</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>0</dep>
      </entry>
    </interpolation>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  
  <!--<animation>
    <type>material</type>
    <condition>
      <greater-than>
        <property>/controls/lighting/panel-norm[1]</property>
        <value>0</value>
      </greater-than>
      <property>/sim/rendering/rembrandt</property>
    </condition>
    <emission>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
      <red>0.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
    </emission>
  </animation>-->

  <animation>
    <type>material</type>
    <condition>
      <greater-than>
        <property>/controls/lighting/panel-norm</property>
        <value>0</value>
      </greater-than>
      <not>
        <property>/sim/rendering/rembrandt</property>
      </not>
    </condition>
    <emission>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
      <red>0.75</red>
      <green>0.25</green>
      <blue>0.25</blue>
    </emission>
  </animation>
  
  <!-- *** LoD *** -->
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5</max-m>
  </animation>
</PropertyList>
