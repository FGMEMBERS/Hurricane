<?xml version="1.0"?>
  <PropertyList> 
    <path>control-stick.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Control-Stick</object-name>
      <property>controls/flight/elevator</property>
      <interpolation> 
        <entry> 
          <ind>-1.0</ind>
          <dep>-12.5</dep>
        </entry>
        <entry> 
          <ind>0.0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1.0</ind>
          <dep>12.5</dep>
        </entry>
      </interpolation>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
    <animation> 
      <type>rotate</type>
      <object-name>Handle</object-name>
      <property>controls/flight/aileron</property>
      <interpolation> 
        <entry> 
          <ind>-1.0</ind>
          <dep>-40</dep>
        </entry>
        <entry> 
          <ind>0.0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1.0</ind>
          <dep>40</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0.42</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
    <animation>
      <type>material</type>
      <condition>
        <greater-than>
          <property>/controls/lighting/panel-norm[1]</property>
          <value>0</value>
        </greater-than>
        <property>/sim/rendering/rembrandt</property>
      </condition>
      <emission>
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.0</red>
        <green>0.0</green>
        <blue>0.0</blue>
      </emission>
    </animation>

    <animation>
      <type>material</type>
      <condition>
        <greater-than>
          <property>/controls/lighting/panel-norm</property>
          <value>0</value>
        </greater-than>
        <not>
          <property>/sim/rendering/rembrandt</property>
        </not>
      </condition>
      <emission>
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
  </PropertyList>
